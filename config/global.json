{
    "Server" : {
        "Port"           : 8085,
        "Name"           : "product-api",
        "DefaultVersion" : "1.0.0",
        "Acceptable"     : [ "application/json" ],
        "ThrottleRate"   : 50,
        "ThrottleBurst"  : 100
    },
    "ServerLogging" : {
        "Name"        : "ServerLogging",
        "Dir"         : "./logs/",
        "File"        : "product-api-server.log"
    },
    "ApplicationLogging" : {
        "Name"        : "ApplicationLogging",
        "Dir"         : "./logs/",
        "File"        : "product-api.log",
        "Level"       : "debug"
    },
    "Data" : {
        "ConnectionString"        : "Driver={SQL Server Native Client 11.0};Server=[SERVER];Database=[DATABASE];UID=[USER];PWD=[PASSWORD]",
        "ProductListSp"        : "[SP]",
        "GroupListSp"        : "[SP]",
        "ContentListSp"        : "[SP]",
        "TitlesSp"        : "[SP]",
        "EdzonesSp"        : "[SP]",
        "ClassificationsSp"        : "[SP]"
    }
}